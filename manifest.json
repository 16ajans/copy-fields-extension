{
  "manifest_version": 2,
  "name": "Eimmet Field Copier",
  "version": "1.0.1",
  "description": "Copy fields between two Firefox windows",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "applications": {
    "gecko": {
      "id": "eimmet-field-copier@mostafa-wahied",
      "update_url": "https://github.com/Mostafa-Wahied/copy-fields-extension/raw/main/update-manifest.json"
    }
  }
}
